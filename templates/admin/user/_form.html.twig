{{ form_start(form) }}
<div class="form-row">
    <div class="form-group col-md-6">
        {{ form_row(form.firstName, {}) }}
    </div>
    <div class="form-group col-md-6">
        {{ form_row(form.lastName, {}) }}
    </div>
</div>

<div class="form-row">
    <div class="form-group col-md-6">
        {{ form_row(form.email, {}) }}
    </div>
    <div class="form-group col-md-6">
        {#{{ form_row(form.AccountStatus) }}#}
        {{ form_rest(form) }}
    </div>
</div>

<button class="btn btn-primary float-right" role="button">{{ button_label|default('common.actions.save'|trans) }}</button>
{{ form_end(form) }}




